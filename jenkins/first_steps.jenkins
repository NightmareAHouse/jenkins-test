#!groovy

properties([disableConcurrentBuilds()])

pipeline {
    agent{
        label 'master'
    }

    options {
        buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
        timestamps()
    }

    stages {
        stage("Build frontend") {
            steps{
                npm install
                npm run build
            }
        }

        stage("Test frontend"){
            npm install
            npm run test
        }
    }
}