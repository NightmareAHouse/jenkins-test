pipeline {
    agent any

    tools {nodejs "node"}

    stages {
        stage('Build') {
            steps {
                git branch: 'main', url: 'https://github.com/NightmareAHouse/jenkins-test.git'
                bat 'npm install'
            }
        }

        stage('Test') {
            git branch: 'main', url: 'https://github.com/NightmareAHouse/jenkins-test.git'
            bat 'npm run test'
        }
    }
}